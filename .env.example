# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# xID API (マイナンバーカード認証)
XID_API_KEY=your_xid_api_key
XID_API_SECRET=your_xid_api_secret

# Stripe Configuration
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key

# WebAuthn/Passkey Configuration
# RP (Relying Party) Name - displayed to users during passkey registration
RP_NAME=親子マッチング

# RP ID - domain name (without protocol or port)
# For development: localhost
# For production: your-domain.com
RP_ID=localhost

# Origin - full URL of your application
# For development: http://localhost:3000
# For production: https://your-domain.com
NEXT_PUBLIC_ORIGIN=http://localhost:3000
